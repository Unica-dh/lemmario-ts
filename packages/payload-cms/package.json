{
  "name": "@lemmario/payload-cms",
  "version": "1.0.0",
  "description": "Payload CMS backend for Lemmario platform",
  "main": "dist/server.js",
  "types": "dist/server.d.ts",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon",
    "build": "tsc",
    "start": "cross-env NODE_ENV=production node dist/server.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist",
    "db:migrate": "payload migrate",
    "db:seed": "ts-node src/seed/index.ts"
  },
  "dependencies": {
    "payload": "^2.8.0",
    "express": "^4.18.2",
    "dotenv": "^16.3.1",
    "cross-env": "^7.0.3",
    "@payloadcms/db-postgres": "^0.8.0",
    "@payloadcms/richtext-lexical": "^0.11.0",
    "@payloadcms/bundler-webpack": "^1.0.7",
    "graphql": "^16.8.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.5",
    "typescript": "^5.3.3",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
