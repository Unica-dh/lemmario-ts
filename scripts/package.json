{
  "name": "@lemmario/scripts",
  "version": "1.0.0",
  "description": "Migration and utility scripts for Lemmario",
  "private": true,
  "scripts": {
    "migrate": "ts-node migration/import.ts",
    "migrate:test": "API_URL=http://localhost:3000/api LEMMARIO_ID=2 ts-node migration/import.ts",
    "move-data": "ts-node migration/move-data.ts",
    "seed:livelli": "ts-node migration/seed-livelli-razionalita.ts",
    "reset:db": "ts-node migration/reset-database.ts",
    "migrate:full": "npm run reset:db && npm run seed:livelli && npm run migrate"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.12",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.19.27",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
